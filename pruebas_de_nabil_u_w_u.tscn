			var object = compuerta.instantiate()
			object.position = tilemap.map_to_local(cellpos) * tilemap.scale
			object.rotar(2)
			add_child(object)
			tilemap.set_cell(0, cellpos, -1)
		if tilemap.get_tipo(cellpos) == 11:
			var object = compuerta.instantiate()
			object.position = tilemap.map_to_local(cellpos) * tilemap.scale
			object.rotar(3)
